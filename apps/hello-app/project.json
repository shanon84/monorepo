{
  "name": "hello-app",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "./hello-app/src",
  "targets": {
    "build-BE": {
      "executor": "@jnxplus/nx-maven:run-task"
    },
    "build-image": {
      "executor": "@jnxplus/nx-maven:run-task",
      "options": {
        "task": "spring-boot:build-image"
      }
    },
    "serve": {
      "executor": "@jnxplus/nx-maven:run-task",
      "options": {
        "task": "spring-boot:run"
      },
      "dependsOn": [
        "^build"
      ]
    },
    "integrationtest-BE": {
      "executor": "@jnxplus/nx-maven:run-task"
    }
  },
  "tags": []
}
